{
    "type": "symfony-bundle",
    "license": "proprietary",
    "name": "savageh/health-check",
    "description": "Health check bundle",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "4.1.*",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/skeleton": "4.1.*",
        "symfony/yaml": "4.1.*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "savageh\\HealthCheckBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "savageh\\HealthCheckBundle\\Tests\\": "tests"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.1.*"
        }
    }
}
